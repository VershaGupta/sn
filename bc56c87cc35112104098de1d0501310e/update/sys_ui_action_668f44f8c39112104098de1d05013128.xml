        <order>100</order>
        <script><![CDATA[function getOAuthTokenFromServiceProvider() {
	g_form.setValue('token_expired', false);
	g_form.save();

	var oauthRequestorSysId = g_scratchpad.restMsgSysID;
	
	var oauthProfileId = g_scratchpad.oauth2_profile;
	var oauthRequestorContext = 'sys_rest_message';

	var oauth_initiator_url = '/oauth_initiator.do' +
		'?oauth_requestor_context=' + oauthRequestorContext +
		'&oauth_requestor=' + oauthRequestorSysId +
		'&oauth_provider_profile=' + oauthProfileId +
		'&response_type=code';
	
	top.window.open(oauth_initiator_url, '', 'height=500,width=800'); // pops up window
	return;
}
]]></script>
