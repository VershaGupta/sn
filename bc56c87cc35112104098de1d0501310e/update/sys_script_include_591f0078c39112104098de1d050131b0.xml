    <sys_script_include action="INSERT_OR_UPDATE">
        <access>package_private</access>
        <active>true</active>
        <api_name>x_snc_egnyte_prote.EgnyteLogger</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Script to handle logging for Egnyte Protect Application</description>
        <mobile_callable>false</mobile_callable>
        <name>EgnyteLogger</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[var EgnyteLogger = Class.create();
EgnyteLogger.prototype = {
    initialize: function() {
    },
	
	type: 'EgnyteLogger'
};

EgnyteLogger.log = function(msg, level) {
	var entry = new GlideRecordSecure('x_snc_egnyte_prote_egnyte_app_logs');
	entry.newRecord();
	entry.setValue('log_message', msg);
	entry.setValue('log_level', level);
	return entry.insert();	
};

EgnyteLogger.error = function(msg) {
	return EgnyteLogger.log(msg, 'ERROR'); 
};

EgnyteLogger.info = function(msg) {
	return EgnyteLogger.log(msg, 'INFO'); 
};

EgnyteLogger.debug = function(msg) {
	return EgnyteLogger.log(msg, 'DEBUG'); 
};]]></script>
